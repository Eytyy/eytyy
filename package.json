{
  "name": "eytyy-tests",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "clean": "rimraf dist",
    "copy:files": "cpy src/**/*.html dist && cpy src/*.ico dist && cpy src/videos/**/*.mp4 dist/videos",
    "copy:dev:data": "cpy src/data/**/*.json dist/data",
    "copy:dev": "npm-run-all copy:*:*",
    "copy": "npm-run-all copy:files",
    "minify-images:desktop": "imagemin './src/images/projects/**/*' --out-dir './dist/images/projects'",
    "minify-images:mobile": "imagemin './src/images/projects-mobile/**/*' --out-dir './dist/images/projects-mobile'",
    "test": "BABEL_ENV=test mocha './src/**/*.spec.js' -w --recursive --compilers js:babel-core/register",
    "start": "nodemon server.js -w",
    "predev": "npm-run-all clean copy:* minify-images:*",
    "dev": "webpack --env.dev --watch",
    "prebuild": "npm-run-all clean copy minify-images:* copy:dev:data",
    "build": "webpack --env.prod -p"
  },
  "keywords": [],
  "author": "Eyas Tayyem <e.tayyem@gmail.com> (http://eytyy.com/)",
  "license": "MIT",
